<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>VirtualProctor</servlet-name>
		<servlet-class>org.teachingkidsprogramming.virtualproctor.VirtualProctorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VirtualProctor</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Image</servlet-name>
		<servlet-class>org.teachingkidsprogramming.virtualproctor.ImageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Image</servlet-name>
		<url-pattern>/Image</url-pattern>
	</servlet-mapping>
	

</web-app>
